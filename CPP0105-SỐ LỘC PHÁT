#include<bits/stdc++.h>
using namespace std;
int lp(string s){
	for(int i=0; i<s.length(); i++){
		if(s[i]!='6' && s[i]!='8' && s[i]!='0'){
			return 0;
		}
		return 1;
	}
}
int main(){
	int t;
	cin>>t;
	cin.ignore();
	while(t--){
		string s;
		getline(cin, s);
        if(lp(s))   cout<<"YES\n";
		else cout<<"NO\n";
	}
} 
